{
    "version":2,
    "builds":[
        {
            "src":"Start/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.11.9"}

        },
        {
            "src":"bulid_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"saticfiles"
            }
        }
        
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"Start/wsgi.py"
        }
    ]    
}